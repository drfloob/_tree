{
    "name": "_tree",
    "version": "0.1.0",
    "description": "A tree data structure and tree behavior library for Javascript",
    "keywords": ["tree", "data structure", "model", "wrapper", "graph"],
    "main": "_tree.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "./node_modules/jasmine-node/bin/jasmine-node tests/",
        "build": "uglifyjs _tree.js -c --mangle --comments \"/\\(c\\)/\" -o _tree.min.js --source-map _tree.min.js.map",
        "docs": "docco _tree.js",
        "lint": "./node_modules/nodelint/nodelint _tree.js tests/ --config .nodelint"
    },
    "dependencies": {
        "underscore": ">=1.5.2"
    },
    "devDependencies": {
        "docco": ">=0.6.2",
        "jasmine-node": ">=1.11.0",
        "uglify-js": ">=2.4.1",
        "nodelint": ">0.0.0"
    },
    "author": "A. J. Heller",
    "license": "MIT"
}
